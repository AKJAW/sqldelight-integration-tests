CREATE TABLE TimestampItemEntity (
id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
name TEXT NOT NULL,
version INTEGER AS Int NOT NULL
);

selectAll:
SELECT * FROM TimestampItemEntity;

insertItem:
INSERT OR IGNORE INTO TimestampItemEntity(name, version)
VALUES (?, 1);

updateTimestamp:
UPDATE TimestampItemEntity SET name = ?, version = version + 1 WHERE id = ?;
